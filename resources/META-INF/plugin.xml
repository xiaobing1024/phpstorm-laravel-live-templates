<idea-plugin>
  <id>com.xiaobing1024.laravel</id>
  <name>Laravel Live Templates</name>
  <version>1.0</version>
  <vendor email="zhuxiaobing1024@gmail.com">xiaobing1024</vendor>

  <description><![CDATA[
      Live Templates For Laravel
      Show details in Preferences -> Editor -> Live Templates
      find code in https://github.com/xiaobing1024/phpstorm-laravel-live-templates
      you can concat me for edit live templates.
    ]]></description>

  <change-notes><![CDATA[
      first submit
    ]]>
  </change-notes>

  <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="173.0"/>

  <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <depends>com.intellij.modules.platform</depends>

  <extensions defaultExtensionNs="com.intellij">
    <defaultLiveTemplates file="/liveTemplates/Laravel-Blade.xml"/>
    <defaultLiveTemplates file="/liveTemplates/Laravel-Collection.xml"/>
    <defaultLiveTemplates file="/liveTemplates/Laravel-Php.xml"/>
  </extensions>

  <actions>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>