<idea-plugin>
  <id>com.xiaobing1024.laravel</id>
  <name>Laravel Live Templates</name>
  <version>1.0</version>
  <vendor email="zhuxiaobing1024@gmail.com">xiaobing1024</vendor>

  <description><![CDATA[
      <h1>Live Templates For Laravel</h1>
      Show details in Preferences -> Editor -> Live Templates-> Laravel-Blade Laravel-Collection Laravel-Php Group.

